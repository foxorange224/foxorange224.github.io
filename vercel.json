{
  "version": 2,
  "headers": [
    {
      "source": "/cdn-test.html",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=3600"
        },
        {
          "key": "CDN-Cache-Control",
          "value": "public, max-age=604800"
        },
        {
          "key": "X-CDN",
          "value": "Vercel-Edge-Network"
        },
        {
          "key": "Via",
          "value": "1.1 vercel"
        }
      ]
    },
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=3600, stale-while-revalidate=86400"
        },
        {
          "key": "CDN-Cache-Control",
          "value": "public, max-age=604800"
        }
      ]
    }
  ],
  "cleanUrls": true,
  "trailingSlash": false
}